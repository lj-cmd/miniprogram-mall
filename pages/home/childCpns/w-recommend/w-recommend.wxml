<!--pages/home/childCpns/w-recommends/w-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{list}}" wx:key="index">
    <view class="recommend-item">
      <image class="img" src="{{item.image}}" bind:load="imageLoad"/>
      <view class=".vie">{{item.title}}</view>
    </view>
  </block>
</view>
